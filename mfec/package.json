{
  "name": "mfec",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev-remote\" \"vite\"",
    "build": "tsc -b && vite build",
    "dev-remote": "npx vite build --watch --config vite.config.remote.js",
    "build-remote": "npx vite build --config vite.config.remote.js",
    "preview-remote": "npx vite preview --port 3003 --config vite.config.remote.js",
    "preview": "npx vite preview",
    "lint": "eslint ."
  },
  "dependencies": {
    "concurrently": "^9.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
